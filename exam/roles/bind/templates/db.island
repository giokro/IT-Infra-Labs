$TTL	604800
@	IN	SOA	island.awsome. root.island.awsome. (
	       {{ ts.stdout_lines[0] }}	; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

{% for i in dns_records %}
{{ i.name }} IN {{ i.type }} {{ i.address }}
{% endfor %}

{% for i in dns_records %}
  {% if i.multi == "yes" %}
   {{ i.name }} {% for address in i %}
                  IN {{ i.type }} {{ i.address }}
                {% endfor %}
  {% endif %}
{% endfor %}

